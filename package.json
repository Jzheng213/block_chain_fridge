{
  "name": "block_chain_fridge",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "webpack": "webpack --watch --config webpack.config.js",
    "webpack-build": "webpack",
    "installs": "npm install && npm install --prefix './frontend'",
    "setup": "npm run installs && npm run setup-db && npm run webpack-build && npm run build --prefix ./frontend",
    "setup-db": "cd server && sequelize db:create && sequelize db:migrate --migrations-path ./migrations",
    "start-server": "nodemon --inspect server/bundle-back.js",
    "start-client": "cd frontend && npm start",
    "start": "concurrently --kill-others \"npm run webpack\" \"npm run start-server\" \"npm run start-client\""
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "connect-session-sequelize": "^5.2.1",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "lodash": "^4.17.10",
    "morgan": "~1.9.0",
    "nodemon": "^1.17.3",
    "normalize.css": "^8.0.0",
    "passport": "^0.4.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "redux-logger": "^3.0.6"
  }
}
